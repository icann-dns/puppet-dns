    {"name":"puppetlabs-concat","version_requirement":">= 1.2.0 <2.0.0"},
    {"name":"icann-nsd","version_requirement":">= 0.2.1 <0.3.0"},
    {"name":"icann-knot","version_requirement":">= 0.2.2 <0.3.0"},
    {"name":"icann-tea","version_requirement":">= 0.2.8 <0.3.0"},
    {"name":"stankevich-python","version_requirement":">= 1.15.0 <2.0.0"}
  ],
  "operatingsystem_support": [
    {
      "operatingsystem" : "Ubuntu",
      "operatingsystemrelease" : [ "14.04" ]
    },
    {
      "operatingsystem" : "FreeBSD",
      "operatingsystemrelease" : [ "10" ]
    }
  ]
