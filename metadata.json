{
  "name": "icann-dns",
  "version": "0.1.11",
  "author": "john.bond@icann.org",
  "summary": "puppet meta module to manage NSD and Knot modules",
  "license": "Apache-2.0",
  "source": "https://github.com/icann-dns/puppet-dns",
  "project_page": "https://github.com/icann-dns/puppet-dns",
  "issues_url": "https://github.com/icann-dns/puppet-dns/issues",
  "tags": [
    "nsd",
    "dns",
    "network"
  ],
  "dependencies": [
    {"name":"puppetlabs-stdlib","version_requirement":">= 4.12.0 <5.0.0"},
    {"name":"puppetlabs-concat","version_requirement":">= 1.2.0 <2.0.0"},
    {"name":"icann-nsd","version_requirement":">= 0.1.8 <0.2.0"},
    {"name":"icann-knot","version_requirement":">= 0.1.3 <1.0.0"},
    {"name":"icann-tea","version_requirement":">= 0.2.5 <1.0.0"},
    {"name":"stankevich-python","version_requirement":">= 1.15.0 <2.0.0"}
  ],
  "operatingsystem_support": [
    {
      "operatingsystem" : "Ubuntu",
      "operatingsystemrelease" : [ "12.0.4", "14.04" ]
    },
    {
      "operatingsystem" : "FreeBSD",
      "operatingsystemrelease" : [ "10" ]
    }
  ]
}

